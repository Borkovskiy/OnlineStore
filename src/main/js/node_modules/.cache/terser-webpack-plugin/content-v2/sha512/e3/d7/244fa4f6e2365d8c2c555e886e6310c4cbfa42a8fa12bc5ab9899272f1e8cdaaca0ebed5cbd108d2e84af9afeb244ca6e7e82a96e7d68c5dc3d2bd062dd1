120.herokuapp.com/store/slider\"\r\n            );\r\n            setImages(result.data)\r\n            setLoading(true)\r\n            setTimeout(() => {\r\n                setLoading(false)\r\n                }, 2000)\r\n        };\r\n        getImages();\r\n    }, []);\r\n\r\n    if (isLoading) {\r\n        return <Backdrop className={classes.backdrop} open>\r\n            <CircularProgress color=\"inherit\" />\r\n        </Backdrop>\r\n    }\r\n\r\n    return (\r\n        <div>\r\n            <Carousel style={{ zIndex: \"3\" }}>\r\n                {\r\n                    images.map(item => (\r\n                        <Carousel.Item key={item.index}>\r\n                            <img\r\n                                className=\"d-block w-100 cover\"\r\n                                src={`data:image/jpeg;base64,${item.image}`}\r\n                                alt=\"First slide\"\r\n                                key={item.index}\r\n                            />\r\n                        </Carousel.Item>\r\n                    ))\r\n                }\r\n            </Carousel>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default CarouselList\r\n\r\n\r\n","import React from \"react\";\r\nimport { makeStyles } from \"@material-ui/core/styles\";\r\nimport Pagination from \"@material-ui/lab/Pagination\";\r\n\r\nconst useStyles = makeStyles((theme) => ({\r\n    root: {\r\n        \"& > *\": {\r\n            marginTop: theme.spacing(2),\r\n            justifyContent:\"center\",\r\n            display:'flex'\r\n        }\r\n    }\r\n}));\r\n\r\nexport const PaginationButtons = ({ handlePageChange }) => {\r\n    const classes = useStyles();\r\n\r\n    return (\r\n        <div className={classes.root}>\r\n            <Pagination\r\n                count={2}\r\n                hidePrevButton\r\n                hideNextButton\r\n                size=\"large\"\r\n                onChange={(e) => handlePageChange(e)}\r\n            />\r\n        </div>\r\n    );\r